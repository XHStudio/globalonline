<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.global.globalonline">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".base.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.mainTab.MainActivity_" />
        <activity android:name=".activities.user.LoginActivity_">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.user.RegisteredActivity_" />
        <activity android:name=".activities.user.ChangeLoginPasswordActivity_" />
        <activity android:name=".activities.user.ChangeTradePasswordActivity_" />
        <activity android:name=".activities.user.ForgotaPsswordActivity_" />

    </application>

</manifest>